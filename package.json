{
    "name": "rust_life",
    "version": "1.0.0",
    "description": "",
    "main": "frontend/index.js",
    "scripts": {
        "prebuild": "npm i -f --no-package-lock --no-shrinkwrap",
        "build": "webpack --config webpack.$ENV.js",
        "clean": "bash clean.sh",
        "predev": "npm run prebuild",
        "dev": "webpack --progress -d -w --config webpack.dev.js",
        "predeploy": "ENV=prod npm run build",
        "deploy": "gh-pages -d dist",
        "install": "npip install -r requirements.txt",
        "prepkg": "npm run build; rm -rf pkg; npm run venv",
        "pkg": "bash setup.sh",
        "start": "nopy run.py -e \"$ENV\"",
        "venv": "nopy -m venv --copies pkg/py_venv"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saminkhan/rust_life.git"
    },
    "author": "Samin Khan",
    "license": "MIT",
    "private": true,
    "bugs": {
        "url": "https://github.com/saminkhan/rust_life/issues"
    },
    "homepage": "https://saminkhan.ca/rust_life",
    "devDependencies": {
        "@babel/core": "^7.8.7",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.8.3",
        "@babel/preset-env": "^7.8.7",
        "@babel/preset-react": "^7.8.3",
        "@wasm-tool/wasm-pack-plugin": "^1.3.1",
        "babel-loader": "^8.0.6",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.4.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-node": "^4.0.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^2.5.1",
        "favicon-hash-webpack-plugin": "^2.0.1",
        "file-loader": "^5.1.0",
        "gh-pages": "^3.1.0",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.9.0",
        "nopy": "^0.2.7",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "url-loader": "^3.0.0",
        "wasm-pack": "^0.9.1",
        "webpack": "^4.42.0",
        "webpack-cli": "^3.3.11",
        "webpack-livereload-plugin": "^2.3.0",
        "webpack-merge": "^4.2.2"
    },
    "dependencies": {
        "bootstrap": "^4.4.1",
        "font-awesome": "^4.7.0",
        "jquery": "^3.4.1",
        "popper.js": "^1.16.1",
        "pure-css-loader": "^3.3.3",
        "react": "^16.13.0",
        "react-dom": "^16.13.0"
    },
    "python": {
        "execPath": "python3"
    }
}
